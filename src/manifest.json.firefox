{
	"manifest_version": 2,
	"name": "JoyReactor Visitor",
	"short_name": "JoyReactor Visitor",
	"description": "",
	"author": "ExtraDJ",
	"version": "0.4.5",
	"homepage_url": "https://patreon.com/extradj",
	"icons": {
		"128": "data/images/logo.png"
	},
	"browser_action": {
		"default_icon": "data/images/enabled.png"
	},
	"permissions": [
		"storage",
		"history",
		"downloads",
		"unlimitedStorage"
	],
	"background": {
		"scripts": ["data/js/script.js"]
	},
	"options_ui": {
		"page": "data/options.html",
		"open_in_tab": true
	},
	"content_scripts": [{
		"matches": [
			"*://joyreactor.cc/*", 
			"*://reactor.cc/*",
			"*://*.reactor.cc/*",
			"*://jr-proxy.com/*",
			"*://jrproxy.com/*"
		],
		"run_at": "document_end",
		"js": ["data/js/content.js"],
		"css": ["data/css/content.css"]
	}],
	"browser_specific_settings": {
		"gecko": {
			"id": "{c9563af4-bd9f-4ac6-a07f-a5f90162d986}"
		}
	}
} 