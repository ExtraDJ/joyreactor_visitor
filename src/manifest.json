{
	"manifest_version": 3,
	"name": "JoyReactor Visitor",
	"short_name": "JoyReactor Visitor",
	"description": "",
	"author": "ExtraDJ",
	"version": "1.0.6",
	"homepage_url": "https://patreon.com/extradj",
	"icons": {
		"128": "data/images/logo.png"
	},
	"action": {
		"default_icon": "data/images/enabled.png"
	},
	"permissions": [
		"tabs",
		"storage",
		"unlimitedStorage",
		"history",
		"downloads",
		"declarativeNetRequest",
		"webRequest"
	],
	"background": {
		"service_worker": "data/js/script.js"
	},
	"options_ui": {
		"page": "data/options.html",
		"open_in_tab": true
	},
	"content_scripts": [{
		"matches": [
			"*://joyreactor.cc/*", 
			"*://*.joyreactor.cc/*", 
			"*://reactor.cc/*",
			"*://*.reactor.cc/*",
			"*://jr-proxy.com/*",
			"*://jrproxy.com/*"
		],
		"run_at": "document_end",
		"js": ["data/js/jquery.js", "data/js/content.js"],
		"css": ["data/css/content.css"]
	}],
	"host_permissions": [
		"*://joyreactor.cc/*", 
		"*://*.joyreactor.cc/*", 
		"*://reactor.cc/*",
		"*://*.reactor.cc/*",
		"*://jr-proxy.com/*",
		"*://jrproxy.com/*"
	],
	"web_accessible_resources": [{ 
		"resources": ["data/templates.html"],
		"matches": [
			"*://joyreactor.cc/*", 
			"*://*.joyreactor.cc/*", 
			"*://reactor.cc/*",
			"*://*.reactor.cc/*",
			"*://jr-proxy.com/*",
			"*://jrproxy.com/*"
		]
	}],
	"minimum_chrome_version": "93.0.46"
}
